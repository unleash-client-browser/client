{
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/unleash-client-browser/client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unleash-client-browser/client.git"
  },
  "keywords": [
    "unleash",
    "client",
    "browser",
    "feature",
    "flag"
  ],
  "bugs": {
    "url": "https://github.com/unleash-client-browser/client/issues"
  },
  "private": false,
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint",
      "commit-msg": "commitlint --edit $1"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@unleash-client-browser/client",
  "author": "Dawid Yerginyan",
  "module": "dist/client.esm.js",
  "size-limit": [
    {
      "path": "dist/client.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/client.esm.js",
      "limit": "10 KB"
    }
  ],
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@size-limit/preset-small-lib": "^4.10.2",
    "commitiquette": "^1.0.11",
    "commitizen": "^4.2.4",
    "husky": "^6.0.0",
    "semantic-release": "^17.4.3",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
